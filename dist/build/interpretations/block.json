{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "vt-music-training/vt-interpretations",
  "version": "0.1.0",
  "title": "Bloc interprétations",
  "category": "widgets",
  "icon": "smiley",
  "description": "Bloc à insérer au bas des modules d'entraînement",
  "supports": {
    "html": false
  },
  "attributes": {
    "interpretations": {
      "type": "string"
    },
    "caracteristiques": {
      "type": "object",
      "properties": {
        "interprete": {
          "type": "string"
        },
        "titre-version": {
          "type": "string"
        },
        "fichier": {
          "type": "string"
        },
        "audio-video": {
          "type": "string"
        },
        "reperes": {
          "type": "string"
        }
      },
      "default": {
        "interprete": "",
        "titre-version": "",
        "fichier": "",
        "audio-video": "video",
        "reperes": ""
      }
    },
    "tstring": {
      "type": "string"
    },
    "selection": {
      "type": "boolean",
      "default": true
    },
    "typeLecteur": {
      "type": "string",
      "default": "audio"
    }
  },
  "textdomain": "vt-music-training",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}