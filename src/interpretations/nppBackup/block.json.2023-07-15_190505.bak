{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "vt-gut-blocks/vt-interpretations",
	"version": "0.1.0",
	"title": "Bloc interprétations",
	"category": "widgets",
	"icon": "smiley",
	"description": "Bloc à insérer au bas des modules d'entraînement",
	"supports": {
		"html": false
	},
	"attributes": {
		 
		"interpretations": {
			"type": "string"
		},
		
		"caracteristiques": {
			"type": "object",
			"properties": {
				"Interprète": {"type": "string"},
				"Titre-Version": {"type": "string"},
				"fichier": {"type": "string"},
				"audiovideo":  {"type": "string"},
				"reperes": {"type": "string"}
			},
			"default": {
				"interprete": "",
				"titreversion": "",
				"fichier": "",
				"audiovideo": "video",
				"reperes": ""
		}
		},

		"tstring": {
			"type": "string"
		},
		
		"selection": {
			"type": "boolean",
			"default": true		
		},
		"typeLecteur": {
			"type": "string",
			"default": "audio"
		}
	},
	"textdomain": "vt-gut-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
