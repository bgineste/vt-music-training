/** Fonctions pour index-oeuvre */

	var adjacentIDs = [];
	function getAdjacentIDs(element) {
		adjacentIDs = [];
		let sibling = element.nextElementSibling;

		while (sibling) {
			adjacentIDs.push(sibling.id);
			sibling = sibling.nextElementSibling;
		}

//		return adjacentIDs;
	}

	export { getAdjacentIDs };